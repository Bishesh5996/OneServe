NODE_ENV=development
PORT=4000
MONGO_URI=mongodb://127.0.0.1:27017/meal-kit
JWT_SECRET=supersecret
# Auth bypass (local/testing only). Set AUTH_BYPASS=false to require tokens.
AUTH_BYPASS=true
AUTH_BYPASS_EMAIL=demo@oneserve.test
AUTH_BYPASS_PASSWORD=changeme
AUTH_BYPASS_ROLE=admin

# Default admin account (created/updated on server start)
ADMIN_EMAIL=admin@oneserve.test
ADMIN_PASSWORD=1234567890
ADMIN_NAME=OneServe Admin
# Set to true to reset admin password to ADMIN_PASSWORD on startup
ADMIN_RESET_PASSWORD=false
